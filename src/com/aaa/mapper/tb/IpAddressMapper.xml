<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="com.aaa.dao.tb.IpAddressDao">
	<insert id="addIp" parameterType="IpAddress">
		insert into ipaddress(IP_address,IP_addtime,p_id,i_id )
		values (#{IP_address},now(),#{p_id},#{i_id})
	</insert>
</mapper>
