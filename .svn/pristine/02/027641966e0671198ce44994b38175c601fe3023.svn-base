package com.aaa.entity;

import java.io.Serializable;
import java.util.Date;

/**
 * 金额表
 * 
 * @author Administrator
 * 
 */
public class AccountBalance implements Serializable {

	private static final long serialVersionUID = 1L;
	private Integer ab_id;
	private Integer u_id;
	private Double ab_money;
	private Double ab_freezeMoney;
	private Date ab_time;

	public Integer getAb_id() {
		return ab_id;
	}

	public void setAb_id(Integer ab_id) {
		this.ab_id = ab_id;
	}

	public Integer getU_id() {
		return u_id;
	}

	public void setU_id(Integer u_id) {
		this.u_id = u_id;
	}

	public Double getAb_money() {
		return ab_money;
	}

	public void setAb_money(Double ab_money) {
		this.ab_money = ab_money;
	}

	public Double getAb_freezeMoney() {
		return ab_freezeMoney;
	}

	public void setAb_freezeMoney(Double ab_freezeMoney) {
		this.ab_freezeMoney = ab_freezeMoney;
	}

	public Date getAb_time() {
		return ab_time;
	}

	public void setAb_time(Date ab_time) {
		this.ab_time = ab_time;
	}

	public AccountBalance(Integer ab_id, Integer u_id, Double ab_money,
			Double ab_freezeMoney, Date ab_time) {
		super();
		this.ab_id = ab_id;
		this.u_id = u_id;
		this.ab_money = ab_money;
		this.ab_freezeMoney = ab_freezeMoney;
		this.ab_time = ab_time;
	}

	public AccountBalance() {
		super();
	}

	@Override
	public String toString() {
		return "AccountBalance [ab_id=" + ab_id + ", u_id=" + u_id
				+ ", ab_money=" + ab_money + ", ab_freezeMoney="
				+ ab_freezeMoney + ", ab_time=" + ab_time + "]";
	}

}
