package com.aaa.entity;

import java.io.Serializable;
import java.util.Date;

/**
 * 营业执照编号
 * 
 * @author Administrator
 * 
 */
public class BusinessLicense implements Serializable {

	private static final long serialVersionUID = 1L;
	private Integer blid;// 编号
	private String name;// 公司名
	private String person;// 公司法人
	private String address;// 公司地址
	private String reg_num;// 执照编号
	private String business;// 经营范围
	private String capital;// 注册资金
	private Date establish_date;// 办理日期
	private Date valid_period;// 到期时间
	private Date addTime;// 上传日期

	public Integer getBlid() {
		return blid;
	}

	public void setBlid(Integer blid) {
		this.blid = blid;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPerson() {
		return person;
	}

	public void setPerson(String person) {
		this.person = person;
	}

	public String getAddress() {
		return address;
	}

	public void setAddress(String address) {
		this.address = address;
	}

	public String getReg_num() {
		return reg_num;
	}

	public void setReg_num(String reg_num) {
		this.reg_num = reg_num;
	}

	public String getBusiness() {
		return business;
	}

	public void setBusiness(String business) {
		this.business = business;
	}

	public String getCapital() {
		return capital;
	}

	public void setCapital(String capital) {
		this.capital = capital;
	}

	public Date getEstablish_date() {
		return establish_date;
	}

	public void setEstablish_date(Date establish_date) {
		this.establish_date = establish_date;
	}

	public Date getValid_period() {
		return valid_period;
	}

	public void setValid_period(Date valid_period) {
		this.valid_period = valid_period;
	}

	public Date getaddTime() {
		return addTime;
	}

	public void setaddTime(Date addTime) {
		this.addTime = addTime;
	}

	@Override
	public String toString() {
		return "BusinessLicense [blid=" + blid + ", name=" + name + ", person="
				+ person + ", address=" + address + ", reg_num=" + reg_num
				+ ", business=" + business + ", capital=" + capital
				+ ", establish_date=" + establish_date + ", valid_period="
				+ valid_period + ", addTime=" + addTime + "]";
	}

	public BusinessLicense(Integer blid, String name, String person,
			String address, String reg_num, String business, String capital,
			Date establish_date, Date valid_period, Date addTime) {
		super();
		this.blid = blid;
		this.name = name;
		this.person = person;
		this.address = address;
		this.reg_num = reg_num;
		this.business = business;
		this.capital = capital;
		this.establish_date = establish_date;
		this.valid_period = valid_period;
		this.addTime = addTime;
	}

	public BusinessLicense() {
		super();
	}

}
