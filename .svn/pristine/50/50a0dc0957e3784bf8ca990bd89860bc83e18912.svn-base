package com.aaa.entity;

import java.util.Date;

/*
 *反馈表 
 * */
public class Feedback {

	private Integer f_id;// 反馈编号
	private String f_content;// 反馈内容
	private Integer u_id;// 反馈人
	private String i_id;// 反馈订单
	private Integer f_state;// 反馈状态：0是已查看，1是未查看
	private Date f_time;// 反馈时间

	public Feedback() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Feedback(Integer f_id, String f_content, Integer u_id, String i_id,
			Integer f_state, Date f_time) {
		super();
		this.f_id = f_id;
		this.f_content = f_content;
		this.u_id = u_id;
		this.i_id = i_id;
		this.f_state = f_state;
		this.f_time = f_time;
	}

	public Integer getF_id() {
		return f_id;
	}

	public void setF_id(Integer f_id) {
		this.f_id = f_id;
	}

	public String getF_content() {
		return f_content;
	}

	public void setF_content(String f_content) {
		this.f_content = f_content;
	}

	public Integer getU_id() {
		return u_id;
	}

	public void setU_id(Integer u_id) {
		this.u_id = u_id;
	}

	public String getI_id() {
		return i_id;
	}

	public void setI_id(String i_id) {
		this.i_id = i_id;
	}

	public Integer getF_state() {
		return f_state;
	}

	public void setF_state(Integer f_state) {
		this.f_state = f_state;
	}

	public Date getF_time() {
		return f_time;
	}

	public void setF_time(Date f_time) {
		this.f_time = f_time;
	}

	@Override
	public String toString() {
		return "Feedback [f_id=" + f_id + ", f_content=" + f_content
				+ ", u_id=" + u_id + ", i_id=" + i_id + ", f_state=" + f_state
				+ ", f_time=" + f_time + "]";
	}

}
