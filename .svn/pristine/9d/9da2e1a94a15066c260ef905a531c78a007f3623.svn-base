package com.aaa.entity;

import java.io.Serializable;
import java.sql.Date;

/**
 * 角色表
 * 
 * @author Administrator
 * 
 */
public class Roless implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private Integer r_id;
	private String r_name;
	private Integer u_id;
	private Integer upu_id;
	private Date r_updateTime;

	public Integer getR_id() {
		return r_id;
	}

	public void setR_id(Integer r_id) {
		this.r_id = r_id;
	}

	public String getR_name() {
		return r_name;
	}

	public void setR_name(String r_name) {
		this.r_name = r_name;
	}

	public Integer getU_id() {
		return u_id;
	}

	public void setU_id(Integer u_id) {
		this.u_id = u_id;
	}

	public Integer getUpu_id() {
		return upu_id;
	}

	public void setUpu_id(Integer upu_id) {
		this.upu_id = upu_id;
	}

	public Date getR_updateTime() {
		return r_updateTime;
	}

	public void setR_updateTime(Date r_updateTime) {
		this.r_updateTime = r_updateTime;
	}

	@Override
	public String toString() {
		return "Roless [r_id=" + r_id + ", r_name=" + r_name + ", u_id=" + u_id
				+ ", upu_id=" + upu_id + ", r_updateTime=" + r_updateTime + "]";
	}

	public Roless(Integer r_id, String r_name, Integer u_id, Integer upu_id,
			Date r_updateTime) {
		super();
		this.r_id = r_id;
		this.r_name = r_name;
		this.u_id = u_id;
		this.upu_id = upu_id;
		this.r_updateTime = r_updateTime;
	}

	public Roless() {
		super();
		// TODO Auto-generated constructor stub
	}

}
