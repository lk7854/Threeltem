<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="com.aaa.dao.tb.PosterCheckDao">
	<insert id="addPc">
		insert into PosterCheck(
			p_id,
			u_id,
			pc_time,
			pc_result,
			pc_remark
		) values (#{p_id},#{u_id},now(),#{pc_result},#{pc_remark})
	</insert>
</mapper>
