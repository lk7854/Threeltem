package com.aaa.entity;

import java.io.Serializable;
import java.util.Date;

/**
 * 消息通知表
 * 
 * @author Administrator
 * 
 */
public class Information implements Serializable {

	private static final long serialVersionUID = 1L;

	private Integer inf_id;
	private String inf_content;
	private Integer mt_id;
	private Date inf_time;
	private Integer u_id;
	private Integer inf_state;

	public Integer getInf_id() {
		return inf_id;
	}

	public void setInf_id(Integer inf_id) {
		this.inf_id = inf_id;
	}

	public String getInf_content() {
		return inf_content;
	}

	public void setInf_content(String inf_content) {
		this.inf_content = inf_content;
	}

	public Integer getMt_id() {
		return mt_id;
	}

	public void setMt_id(Integer mt_id) {
		this.mt_id = mt_id;
	}

	public Date getInf_time() {
		return inf_time;
	}

	public void setInf_time(Date inf_time) {
		this.inf_time = inf_time;
	}

	public Integer getU_id() {
		return u_id;
	}

	public void setU_id(Integer u_id) {
		this.u_id = u_id;
	}

	public Integer getInf_state() {
		return inf_state;
	}

	public void setInf_state(Integer inf_state) {
		this.inf_state = inf_state;
	}

	public Information(Integer inf_id, String inf_content, Integer mt_id,
			Date inf_time, Integer u_id, Integer inf_state) {
		super();
		this.inf_id = inf_id;
		this.inf_content = inf_content;
		this.mt_id = mt_id;
		this.inf_time = inf_time;
		this.u_id = u_id;
		this.inf_state = inf_state;
	}

	public Information() {
		super();
	}

	@Override
	public String toString() {
		return "Information [inf_id=" + inf_id + ", inf_content=" + inf_content
				+ ", mt_id=" + mt_id + ", inf_time=" + inf_time + ", u_id="
				+ u_id + ", inf_state=" + inf_state + "]";
	}
}
